{{>layout/header}}
<div class="col-md-4">
    <h2>Stock Information</h2>
    <div class="today-date">
        <text>{{date}}</text>
    </div>

    <br><br>
    <span>
        <input type="text" class="form-control" id="stock_name" placeholder="enter stock name"><br>
        <button id="btn-stock" class="btn btn-primary">search</button>
    </span>

    <br><br><br>
</div>

    <br><br><br>

<div class="col-md-6">
    <h5>Result</h5>

    <div class="result">
        <ul id="code">Code : {{stock.code}}</ul>
        <ul id="stock_name">Stock : {{stock.stockName}}</ul>
        <ul id="curr_price">Current Price : {{stock.currPrice}}</ul>
    </div>

    <br>

    <h5>Prediction Result</h5>

    <div class="prediction">
        <ul>> {{prediction}}</ul>
        <ul>> Prediction Price : {{stock.prePrice}}</ul>
    </div>

</div>



{{>layout/footer}}